			"properties": {
				"annotations": [],
				"type": "HttpServer",
				"typeProperties": {
					"url": "https://kesz.sharepoint.com/sites/BIportal2/ETL_landingzone",
					"enableServerCertificateValidation": true,
					"authenticationType": "Windows",
					"userName": "bicskey.simon@kesz.hu",
					"password": {
						"type": "SecureString",
						"value": "**********"
					}
				}
			},
			"dependsOn": []
